<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Allow basic app data transfer but exclude sensitive files -->
        <exclude domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>